    {{#each notes as |note|}}
    <div class="pure-g">
        <div class="pure-u-23-24 center-item-h">
        <h3>{{note.name}}</h3>
        <p>{{note.description}}</p>
      </div>
    </div>
  {{/each}}

{{#each users as |user| }}
<div class="row align-center mb-5">
  <div class="col-md-6">
    <h2>{{{get_emoji}}} {{user.name}}</h2>
  </div>
  <div class="col-md-6">
    <h3><a href="mailto:{{user.email}}">{{user.email}}</a></h3>
  </div>
</div>
{{/each}}
